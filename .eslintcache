rapperCol: { span: 16 },\n  };\n\n  const onFinish = (values) => {\n    create(values).then(response => {\n      if(response){\n        setCreatedSuccessfully(true);\n      } else {\n        setCreatedSuccessfully(false);\n      }\n      setFisrtRender(false);\n      form.resetFields();\n    })\n  }\n\n  return (\n    <div>\n      <h2>{props.name}</h2>\n    <Form {... layout} form={form} name=\"create\" onFinish={onFinish} validateMessages={validateMessages}>\n      <Form.Item name=\"name\" label=\"Nome\" rules={[{ required: true }]}>\n        <Input />\n      </Form.Item>\n      <Form.Item name=\"email\" label=\"Email\" rules={[{ required: true }]}>\n        <Input type=\"email\"/>\n      </Form.Item>\n      <Form.Item name=\"age\" label=\"Idade\" rules={[{ required: true }]}>\n        <Input type=\"number\"/>\n      </Form.Item>\n      <Form.Item name=\"address\" label=\"Endereço\" rules={[{ required: true }]}>\n        <Input />\n      </Form.Item>\n      <Form.Item name=\"cpf\" label=\"Cpf\" rules={[{ required: true }]}>\n        <Input size=\"11\" maxLength=\"11\"/>\n      </Form.Item>\n      { isUser &&\n        <div>\n          <Form.Item name=\"login\" label=\"Login\" rules={[{ required: true }]}>\n            <Input />\n          </Form.Item>\n          <Form.Item name=\"password\" label=\"Senha\" rules={[{ required: true }]}>\n            <Input.Password\n              iconRender={visible => (visible ? <EyeTwoTone /> : <EyeInvisibleOutlined />)}\n            />\n          </Form.Item>\n        </div>\n      }\n      <Form.Item wrapperCol={{ ...layout.wrapperCol, offset: 2 }}>\n        <Button type=\"primary\" htmlType=\"submit\">\n          Criar\n        </Button>\n      </Form.Item>\n    </Form>\n    {createdSuccessfully &&\n      <h2 style={{margin: '10px 0px'}}>{isUser ? 'Usuário' : 'Cliente'} criado com sucesso!</h2>\n    }\n    {!firstRender && !createdSuccessfully &&\n      <h2 style={{margin: '10px 0px'}}>Erro ao criar o {isUser ? 'usuário' : 'cliente'}!</h2>\n    }\n    </div>\n  )\n}\n\nexport default CreateForm;","/home/otavio/Desktop/crudProjeto/frontend/src/services/create.js",[],"/home/otavio/Desktop/crudProjeto/frontend/src/components/layout/Content.jsx",[],"/home/otavio/Desktop/crudProjeto/frontend/src/components/layout/SideBar.jsx",[],"/home/otavio/Desktop/crudProjeto/frontend/src/components/layout/MenuHeader.jsx",[],"/home/otavio/Desktop/crudProjeto/frontend/src/components/FindPerson.jsx",["100"],"import React, { useState } from 'react'\nimport { Form, Input, Card, Row, Button, Radio } from 'antd';\nimport { SearchOutlined } from '@ant-design/icons';\nimport {searchUserByName, searchUserById, searchAllUsers, searchClientByName, searchClientById, searchAllClients} from '../services/search'\n\nconst FindPerson = (props) => {\n  const [find, setFind] = useState('all');\n  const [persons, setPersons] = useState(null);\n  const [firstRender, setFirstRender] = useState(true);\n  const isUser = props.name.includes('Usuário');\n\n  const validateMessages = {\n    required: '${label} é obrigatório!'\n  }\n\n  const handleSearchUser = async (value) => {\n    if(value.name) {\n      await searchUserByName(value.name).then(response => setPersons(response));\n    } else if(value.id) {\n      await searchUserById(value.id).then(response => setPersons(response ? [response]: undefined));\n    } else {\n      await searchAllUsers().then(response => setPersons(response));\n    }\n    setFirstRender(false);\n  }\n\n  const handleSearchClient = async (value) => {\n    if(value.name) {\n      await searchClientByName(value.name).then(response => setPersons(response));\n    } else if(value.id) {\n      await searchClientById(value.id).then(response => setPersons(response ? [response]: undefined));\n    } else {\n      await searchAllClients().then(response => setPersons(response));\n    }\n    setFirstRender(false);\n  }\n  \n  return (\n    <div>\n      <Row>\n        <h2>{props.name}</h2>\n      </Row>\n      <Row>\n        <Radio.Group>\n          <Radio.Button onClick={() => setFind('all')} value=\"all\">Procurar todos</Radio.Button>\n          <Radio.Button onClick={() => setFind('byName')} value=\"byName\">Procurar por nome</Radio.Button>\n          <Radio.Button onClick={() => setFind('byId')} value=\"byId\">Procurar por id</Radio.Button>\n        </Radio.Group>\n      </Row>\n        <Row>\n          <Form onFinish={isUser ? handleSearchUser : handleSearchClient} validateMessages={validateMessages}>\n            { find === 'byName' &&\n              <Form.Item name=\"name\">\n                <Input placeholder=\"Nome\" style={{margin: '10px 0px 0px 0px'}} rules={[{ required: true }]}/>\n              </Form.Item>\n            }\n            { find === 'byId' &&\n              <Form.Item name=\"id\">\n                <Input type=\"number\" placeholder=\"Id\" style={{margin: '10px 0px 0px 0px'}} rules={[{ required: true }]}/>\n              </Form.Item>\n            }\n            <Form.Item>\n              <Button style={{margin: '10px 0px 0px 0px'}} type=\"primary\" icon={<SearchOutlined />} htmlType=\"submit\">\n                Procurar\n              </Button>\n            </Form.Item>\n          </Form>\n        </Row>\n        <Row>\n          { persons &&\n            persons.map((person) => {\n              return(\n                <Card size=\"small\" style={{width: 300,margin: '10px', textAlign: 'center'}} key={person.id}>\n                  <p><b>Id: </b>{person.id}</p>\n                  <p><b>Nome: </b>{person.fullName}</p>\n                  <p><b>Email: </b>{person.email}</p>\n                  <p><b>Idade: </b>{person.age}</p>\n                </Card>\n              )\n            })\n          }\n          { !firstRender && !persons &&\n            <h2>Nenhum Usuário encontrado!</h2>\n          }\n        </Row>\n    </div>\n  )\n}\n\nexport default FindPerson;","/home/otavio/Desktop/crudProjeto/frontend/src/services/search.js",[],"/home/otavio/Desktop/crudPr